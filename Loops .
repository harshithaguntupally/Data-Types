for in :
******

PROGRAM 1 :

 const a = "Harshitha"
        for (let i in a){
            console.log(a[i]);
            document.write(a[i]);
        }
        
OUTPUT :

Harshitha

 
WHILE LOOP :
--for loop
--infinite(unknown)
SYNTAX:
While(condition) {
Block of code
}

EX :
 

let i=1, n=5;
while(i<=n){
console.log(i+1)
i=i+2
}


Let i=1 , n=5;
While(i<=n) {
//1<=5
Console.log(i+1) 
//1,2
i=i+1 
//1+1=2;3,4,5,6
}

OUTPUT : 246


 let i=2, n=8;
