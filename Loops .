console.log(i+1)
document.write(i+1)
i=i+2
}

OUTPUT : 3579


//while loop

        document.write("while loop","<br>")
        let i=1, n=5;
        while(i<=n){
            document.write(i,"<br>")
            i=i+1
        }
        let k=2,l=8;
        while(n<=l){
            document.write(n,"<br>")
            n=n+2
        }

